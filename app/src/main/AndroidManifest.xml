<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apartment_laundry_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Apartmentlaundryapp">
        <activity
            android:name=".TestingFeatures"
            android:exported="false"
            android:label="@string/title_activity_testing_features"
            android:theme="@style/Theme.Apartmentlaundryapp" />
        <activity
            android:name=".LogInPortal"
            android:exported="false"
            android:label="@string/title_activity_log_in_portal"
            android:theme="@style/Theme.Apartmentlaundryapp" />
        <activity
            android:name=".RegistrationSuccessful"
            android:exported="false"
            android:label="@string/title_activity_registration_successful"
            android:theme="@style/Theme.Apartmentlaundryapp" />
        <activity
            android:name=".Registration"
            android:exported="false"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.Apartmentlaundryapp" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>